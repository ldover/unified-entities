{
  "name": "unified-entities",
  "version": "0.1.1",
  "type": "module",
  "author": "Luka Dover <luka.dover@gmail.com>",
  "license": "UNLICENSED",
  "description": "Object graph for the unified data system",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldover/unified-entities.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "tsc -p tsconfig.json",
    "pack": "npm run clean && npm run build && npm pack",
    "test": "vitest"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "moment": "^2.30.1",
    "uuid": "^9.0.1",
    "@types/unist": "^3.0.2",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unist-util-find": "^3.0.0",
    "unist-util-is": "^6.0.0",
    "unist-util-visit-parents": "^6.0.1"
  }
}
